{
  "name": "simple-breakpoints",
  "version": "1.1.6",
  "description": "A simple js plugin to define conditionally check defined breakpoints",
  "main": "dist/index.js",
  "types": "./types/index.d.ts",
  "scripts": {
    "compile": "babel --presets=es2015 -d dist src",
    "prepublish": "npm run compile",
    "test": "ava"
  },
  "author": "Drew J Bartlett",
  "license": "ISC",
  "keywords": [
    "breakpoints",
    "js",
    "mobile",
    "vue"
  ],
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "ava": {
    "require": [
      "babel-register"
    ]
  },
  "devDependencies": {
    "ava": "^0.18.1",
    "babel-cli": "^6.18.0",
    "babel-preset-es2015": "^6.18.0",
    "browser-env": "^2.0.21"
  },
  "bugs": {
    "url": "https://github.com/drewjbartlett/simple-breakpoints/issues"
  },
  "homepage": "https://github.com/drewjbartlett/simple-breakpoints#readme",
  "dependencies": {
    "eventemitter3": "^3.1.0"
  }
}
